{
    "configurations": [
        {
            "name": "C++ Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
            ],
            "preLaunchTask": "GCC Build"
        },
        {
            "name": "Go Debug",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
        },
        {
            "name": "Python Debug",
            "type": "python",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Lua Debug",
            "type": "lua",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "C# Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "C# Build",
            "program": "${workspaceFolder}/csharp/bin/Debug/net6.0/csharp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/csharp",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
    ],
    "version": "2.0.0"
}